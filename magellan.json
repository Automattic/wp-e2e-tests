{
  "mocha_tests": [
    "specs-video"
  ],
  "mocha_opts": "test/mocha.opts",
  "mocha_args": "-R spec-xunit-reporter",
  "max_test_attempts": 3,
  "max_workers": 6,
  "suiteTag": "parallel",
  "sauce": false,
  "browser": "chrome",
  "framework": "testarmada-magellan-mocha-plugin",
  "reporters": [
		"./lib/reporter/magellan-reporter.js",
		"./lib/reporter/screencast-reporter.js"
  ],
	"bail_time": 300000
}
