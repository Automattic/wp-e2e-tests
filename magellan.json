{
  "mocha_tests": [
    "specs/*log*"
  ],
  "mocha_opts": "test/mocha.opts",
  "_mocha_args": "-R spec-xunit-slack-reporter",
  "mocha_args": "--reporter json",
  "max_test_attempts": 2,
  "max_workers": 3,
  "suiteTag": "parallel",
  "sauce": false,
  "browser": "chrome",
  "framework": "testarmada-magellan-mocha-plugin",
  "reporters": [
	  "./lib/reporter/magellan-reporter.js",
	  "testarmada-magellan-xunit-reporter"
  ]
}
